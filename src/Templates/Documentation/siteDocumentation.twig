            popupWin.document.open()
            popupWin.document.write('<html><head><link rel="stylesheet" type="text/css" href="{{ app.request.basepath }}assets/dist/css/print.css" /><link rel="stylesheet" type="text/css" href="{{ app.request.basepath }}assets/bootstrap/css/bootstrap.min.css" /></head><body onload="window.print();window.close();">' + isi.innerHTML + '</body></html>');
            popupWin.document.close();
        }
